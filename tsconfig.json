{
  "include": ["src"],
  "compilerOptions": {
    "types": ["vite/client"],
    "lib": ["dom", "es2024"],
    "target": "es2023",
    "sourceMap": true,
    "noEmit": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true
  }
}
