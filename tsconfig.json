{
  "include": ["src"],
  "compilerOptions": {
    "types": ["vite/client"],
    "lib": ["dom", "es2023"],
    "target": "es2022",
    "sourceMap": true,
    "noEmit": true,
    "jsx": "react",
    "module": "es2022",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  }
}
